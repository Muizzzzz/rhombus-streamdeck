  "compilerOptions": {
    "target": "ES2022",
    "module": "Node16",
    "rootDir": "src",
    "outDir": "com.muizz.rhombus-camera.sdPlugin/bin",
    "strict": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "resolveJsonModule": true,
    "moduleResolution": "node16",
    "lib": ["es2020","dom"],
    "baseUrl": ".",
    "paths": {
      "@elgato/schemas/streamdeck/plugins": [
        "./node_modules/@elgato/schemas/dist/streamdeck/plugins/index"
      ]
    }
  },
  "include": ["src"]
}
